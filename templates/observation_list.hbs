{% extends "site/base.hbs" %}
{% block content %}
  <dl id="observation_list" class="dl-horizontal">
    {% for obs in observation_list %}
      <dt>{{ obs['_id'] }}</dt> 
      <dd>{{ obs['date'] }}</dd> 
      <dd>Exposures: {{ obs['count'] }}</dd> 
	  <dd>Time: {{ obs['exp_time'] / 60 }} min.</dd>      
    {% end %}
  </dl>
{% end %}